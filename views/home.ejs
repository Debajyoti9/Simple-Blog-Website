
<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= HomePageContent %></p>
 <% posts.forEach((element)=>{ %>
    <h1><%= element.title %></h1>
    <% let str = element.content; %>
    <% if(str.length > 150){ %>
      <%  str = str.substring(0,150) +"..."; } %>
    <p><%= str %> <a href="/posts/<%= element._id %>">read more...</a></p>
   
 <% }) %>

<%- include('partials/footer'); -%>